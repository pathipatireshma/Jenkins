pipeline {
    agent none
    stages {

        stage('terraform INIT') {
            agent node ( lebel 'workstation' )
            steps {
                   sh 'echo Hello World'
            }
        }
        stage('terraform PLAN') {
            steps {
                   sh 'echo Hello World'
            }
        }
        stage('terraform APPLY') {
             steps {
                   sh 'echo Hello World'
             }
        }
    }
}